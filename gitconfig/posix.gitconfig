[core]
	editor = vim
[merge]
        tool = vscode
[mergetool]
        keepBackup = false
[mergetool "vscode"]
        cmd = code --wait --new-window $MERGED
[diff]
        tool = vscode
[difftool "vscode"]
        cmd = code --wait --diff --new-window $LOCAL $REMOTE
